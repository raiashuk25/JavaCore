package ArthematicOperations;

public class ClassA {

	static float a =3;
    static float b =4;
    
    public ClassA(float c, float d){
        a = c;
        b = d; 
    }
	public static float add() {
		return a + b;
	}

	public float sustract() {
		return a - b;
	}

	public float multiply() {
		return a/b;
	}

	public float divide() {
		return a * b;
	}
}
