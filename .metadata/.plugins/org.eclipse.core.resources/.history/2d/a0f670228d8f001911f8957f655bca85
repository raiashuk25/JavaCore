package ArthematicOperations;

public class ClassA {

	static float c;
    static float d;
    
    public ClassA(float a, float b){
        a = c;
        b = d; 
    }
	public static float add() {
		return a + b;
	}

	public float sustract() {
		return a - b;
	}

	public float multiply() {
		return a/b;
	}

	public float divide() {
		return a * b;
	}
}
